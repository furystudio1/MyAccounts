<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:state_pressed="false"
        android:state_focused="false" android:color="#64f1f1f1">
    </item>

    <!-- button focused -->
    <item
        android:state_pressed="false"
        android:state_focused="true" android:color="#f1f1f1">
    </item>

    <!-- button pressed -->
    <item
        android:state_pressed="true"
        android:state_focused="false" android:color="#f1f1f1">
    </item>

</selector>